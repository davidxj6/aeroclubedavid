

<h1>Lista de Alunos</h1>
<table class="table table-hover" style="widht:100%">


  <thead>
    <tr>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>CPF</th>
      <th>Endereço</th>
      <th>Profissão</th>
      <th>Email</th>
      <th>CNH</th>
      <th>Escolaridade</th>
      <th>Instituição</th>
      <th>Curso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.Nome %></td>
        <td><%= aluno.sobrenome %></td>
        <td><%= aluno.cpf %></td>
        <td><%= aluno.endereco %></td>
        <td><%= aluno.profissao %></td>
        <td><%= aluno.email %></td>
        <td><%= aluno.cnh %></td>
        <td><%= aluno.escolaridade %></td>
        <td><%= aluno.instituicao %></td>
        <td><%= aluno.curso %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Aluno', new_aluno_path %>
